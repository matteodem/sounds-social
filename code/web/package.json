{
  "name": "sounds-social-web",
  "version": "1.0.0",
  "description": "Web UI for Sounds Social",
  "license": "GPL-3.0",
  "author": "Matteo De Micheli <matteodem@protonmail.com>",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "npm run dev",
    "build": "node build/build.js",
    "unit": "BABEL_ENV=test jest src/**",
    "unit-watch": "BABEL_ENV=test jest --watch src/**",
    "test": "npm run unit",
    "cli": "npm run lint && npm run test",
    "lint": "eslint --ext .js,.vue src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-dev": "npm run build && cd dist && mv index.html 200.html && surge --domain https://sounds-social-dev.surge.sh .",
    "format": "prettier-eslint --write 'src/**/*.js'",
    "precommit": "lint-staged"
  },
